    "rich_text_yHRypG": {
      "type": "rich-text",
      "blocks": {
        "heading_8pGFYA": {
          "type": "heading",
          "settings": {
            "heading": "Fair Wages",
            "heading_size": "h1",
            "select_icon": "shopify:\/\/shop_images\/money-manager-svgrepo-com_1.svg"
          }
        },
        "text_9GjFWA": {
          "type": "text",
          "settings": {
            "text": "<p>At <strong>aoho!<\/strong>, we strongly believe in providing fair wages to our artists, ensuring that they have a sustainable livelihood while pursuing their craft. We strive to create a sense of community among our artists and are committed to paying them fairly without any negotiations. This not only enriches their art but also helps to uplift their lives and the lives of their families.<\/p>"
          }
        }
      },
      "block_order": [
